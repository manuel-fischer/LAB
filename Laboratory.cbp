					<Add option="-g" />
				</Compiler>
				<ExtraCommands>
					<Add after="cmd /c objdump -D bin\Release\Laboratory.exe &gt; whole.dis" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="Release -- ASM">
				<Option output="bin/Release/Laboratory" prefix_auto="1" extension_auto="1" />
				<Option object_output="asm/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Ofast" />
					<Add option="-S" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
